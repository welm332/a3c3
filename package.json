{
  "name": "auto3",
  "version": "1.3.0",
  "main": "./src/main.js",
  "description": "Editor for Python",
  "author": "welm",
  "build": {
    "appId": "com.example.myapp",
    "productName": "Auto3",
    "files": [
      "**/*"
    ]
  },
  "scripts": {
    "start": "electron .",
    "install":"electron installer/main.js",
    "build:inst:win": "electron-builder   -w --x64",
    "build:inst:mac": "electron-builder   -m --x64",
    "build:inst:linux": "electron-builder   -l --x64",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "dir && setx kaminoko kaminoko"
  },
  "devDependencies": {
    "electron": "^13.2.1",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "ace-builds": "^1.4.12",
    "electron-args": "^0.1.0",
    "electron-json-storage": "^4.5.0",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.4.1",
    "electron-winstaller": "^5.0.0",
    "encoding-japanese": "^1.0.30",
    "fs": "^0.0.1-security",
    "pyright": "^1.1.162",
    "sudo-prompt": "^9.2.1",
    "windows-shortcuts-ps": "^1.0.1"
  },
  "private": true
}
